@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AutocompleteEngine" as AutocompleteEngine LOGIC_COLOR
end box

box UI UI_COLOR_T1
participant ":CommandBox" as CommandBox UI_COLOR
end box

[-> CommandBox : user presses TAB
activate CommandBox

CommandBox -> LogicManager : autocompleteCommand(userInput, commandSuggestion)
activate LogicManager

LogicManager -> AutocompleteEngine : autocompleteCommand(userInput, commandSuggestion)
activate AutocompleteEngine

AutocompleteEngine --> LogicManager : autocompletedCommand
deactivate AutocompleteEngine

LogicManager --> CommandBox : autocompletedCommand
deactivate LogicManager

CommandBox --> UI : update user input
deactivate CommandBox
@enduml
